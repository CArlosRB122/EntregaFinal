{
    "name": "trabajo-final",
    "version": "1.0.0",
    "description": "Proyecto para integrar Codecov y Sentry",
    "main": "index.js",
    "scripts": {
      "test": "jest",
      "coverage-report": "jest --coverage", 
      "start": "node index.js",
      "build": "echo 'Construyendo proyecto...'",
      "lint": "eslint .",
      "upload-coverage": "jest --coverage && codecov" 
    },
    "dependencies": {
      "@sentry/node": "^7.66.1"
    },
    "devDependencies": {
      "jest": "^29.7.0",
      "eslint": "^8.52.0",
      "codecov": "^3.8.3"
    },
    "jest": {
      "collectCoverage": true,
      "coverageDirectory": "coverage",
      "coverageThreshold": {
        "global": {
          "branches": 70,
          "functions": 70,
          "lines": 70,
          "statements": 70
        }
      }
    },
    "author": "Carlos Romero",
    "license": "MIT"
  }